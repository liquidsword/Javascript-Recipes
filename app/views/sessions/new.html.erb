<h2>Sign in </h2>

<%= form_for @culinary_artist do |f| %>

  <div class="form-group">
    <%= f.label :name, "Enter your name" %><br>
    <%= f.text_field :culinary_artist_name %>
  </div>

  <div class="form-group">
    <%= f.label :password, "Please enter your password" %><br>
    <%= f.password_field :password %>
  </div>

  <div class="actions">
    <%= f.submit "Sign In", class: "btn btn-primary" %>
  </div>
<% end %>
