<%= form_for (signin_path) do |f| %>

  <div class="form-group">
    <%= f.label :name, "Enter your name" %><br>
    <%= f.text_field :culinary_artist_name %>
  </div>

  <div class="form-group">
    <%= f.label :password, "Please enter your password" %><br>
    <%= f.password_field :password %>
  </div>

  <div class="actions">
    <%= f.submit "Log In", class: "btn btn-primary" %>
  </div>
<% end %>
